{
  "name": "ws-ipfs",
  "version": "1.0.0",
  "private": true,
  "workspaces": [
    "packages/@lazy-ipfs/*",
    "packages/*"
  ],
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "lerna:publish": "npx lerna publish",
    "lerna:publish:yes": "npx lerna publish --yes --cd-version patch",
    "prepublish:lockfile": "npx sync-lockfile .",
    "ncu": "npx yarn-tool ncu -u && npx yarn-tool ws exec yarn-tool ncu -- -u",
    "sort-package-json": "npx yarn-tool sort && npx yarn-tool ws exec yarn-tool sort"
  },
  "resolutions": {},
  "dependencies": {
    "bluebird": "^3.7.2",
    "cross-fetch": "^3.0.5",
    "fs-extra": "^9.0.1",
    "ipfs": "^0.48.0",
    "ipfs-http-client": "^45.0.0",
    "ipfs-utils": "^2.3.1",
    "ipfsd-ctl": "^5.0.0",
    "jest": "^26.1.0",
    "ts-jest": "^26.1.3"
  },
  "devDependencies": {
    "@bluelovers/tsconfig": "^1.0.20",
    "@types/bluebird": "^3.5.32",
    "@types/chrome": "^0.0.120",
    "@types/fs-extra": "^9.0.1",
    "@types/jest": "^26.0.4",
    "@types/lodash": "^4.14.157",
    "@types/node": "^14.0.23",
    "@types/node-fetch": "*",
    "@types/yargs": "^15.0.5"
  },
  "peerDependencies": {
    "lerna": "^3"
  }
