{
  "name": "ws-ipfs",
  "version": "1.0.0",
  "private": true,
  "keywords": [
    "IPFS",
    "ipfs",
    "multihash",
    "multiformats",
    "hash",
    "multibase",
    "encode",
    "decode",
    "formats",
    "libp2p",
    "datastore",
    "unixfs",
    "cid",
    "dweb",
    "node",
    "webrtc",
    "swarm",
    "pubsub",
    "p2p",
    "ipns",
    "ipld"
  ],
  "workspaces": [
    "packages/@lazy-ipfs/*",
    "packages/*"
  ],
  "scripts": {
    "test": "echo \"Error: no test specified\"",
    "lerna:publish": "yarn run prepublishOnly:root && lerna publish && yarn run postpublishOnly",
    "lerna:publish:yes": "yarn run prepublishOnly:root && lerna publish --yes --bump patch && yarn run postpublishOnly",
    "preversion": "yt ws run test",
    "prepare:fix-ws-links": "ynpx --quiet @yarn-tool/fix-ws-links",
    "prepublishOnly:check-bin": "ynpx --quiet @yarn-tool/check-pkg-bin",
    "prepublishOnly:lockfile": "ynpx --quiet sync-lockfile",
    "prepublishOnly:root": "yarn run prepublishOnly:check-bin",
    "postpublishOnly": "echo postpublishOnly",
    "ncu": "yarn run ncu:root && yarn run ncu:ws",
    "ncu:root": "yarn-tool ncu -u",
    "ncu:ws": "yarn-tool ws exec yarn-tool ncu -- -u",
    "sort-package-json": "yarn run sort-package-json:root && yarn run sort-package-json:ws",
    "sort-package-json:root": "yarn-tool sort",
    "sort-package-json:ws": "yarn-tool ws exec yarn-tool sort"
  },
  "resolutions": {},
  "dependencies": {
    "bluebird": "^3.7.2",
    "cross-fetch": "^3.1.4",
    "fs-extra": "^10.0.0",
    "ipfs": "^0.56.1",
    "ipfs-core-types": "^0.6.1",
    "ipfs-http-client": "^51.0.1",
    "ipfs-utils": "^8.1.4",
    "ipfsd-ctl": "^10.0.1",
    "is-ipfs": "^6.0.1",
    "jest": "^27.0.6",
    "jest-node-exports-resolver": "^1.0.3",
    "scramjet": "^4.35.20",
    "ts-jest": "^27.0.4"
  },
  "devDependencies": {
    "@bluelovers/tsconfig": "^1.0.25",
    "@types/bluebird": "^3.5.36",
    "@types/chrome": "^0.0.149",
    "@types/fs-extra": "^9.0.12",
    "@types/jest": "^26.0.24",
    "@types/lodash": "^4.14.171",
    "@types/micromatch": "^4.0.2",
    "@types/node": "^16.4.7",
    "@types/node-fetch": "*",
    "@types/yargs": "^17.0.2",
    "tslib": "^2.3.0"
  },
  "peerDependencies": {
    "lerna": "^4"
  }
}
