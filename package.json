{
  "name": "ws-ipfs",
  "version": "1.0.0",
  "private": true,
  "workspaces": [
    "packages/@lazy-ipfs/*",
    "packages/*"
  ],
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "lerna:publish": "npx lerna publish",
    "lerna:publish:yes": "npx lerna publish --yes --cd-version patch",
    "prepublish:lockfile": "npx sync-lockfile .",
    "ncu": "npx yarn-tool ncu -u && npx yarn-tool ws exec yarn-tool ncu -- -u",
    "sort-package-json": "npx yarn-tool sort && npx yarn-tool ws exec yarn-tool sort"
  },
  "resolutions": {
    "ipfs-http-client": ">= 43.0.0",
    "ipfs-repo": ">= 0.30.1",
    "ipfs-unixfs-importer": ">= 0.44.1",
    "ipfs-utils": ">= 0.7.2",
    "is-ipfs": ">= 0.6.3"
  },
  "dependencies": {
    "bluebird": "^3.7.2",
    "cross-fetch": "^3.0.5",
    "fs-extra": "^9.0.1",
    "ipfs": "^0.46.0",
    "ipfs-http-client": "^44.2.0",
    "ipfs-utils": "^2.3.0",
    "ipfsd-ctl": "^4.1.1",
    "jest": "^26.0.1",
    "ts-jest": "^26.1.0"
  },
  "devDependencies": {
    "@bluelovers/tsconfig": "^1.0.19",
    "@types/bluebird": "^3.5.32",
    "@types/chrome": "^0.0.115",
    "@types/fs-extra": "^9.0.1",
    "@types/jest": "^26.0.0",
    "@types/lodash": "^4.14.155",
    "@types/node": "^14.0.13",
    "@types/node-fetch": "*",
    "@types/yargs": "^15.0.5"
  },
  "peerDependencies": {
    "lerna": "^3"
  }
}
