{
  "name": "@bluelovers/ipfs-http-client",
  "version": "1.0.93",
  "description": "ipfs-http-client with auto search go-ipfs, js-ipfs",
  "keywords": [
    "ipfs",
    "go-ipfs",
    "js-ipfs",
    "promise",
    "async"
  ],
  "homepage": "https://github.com/bluelovers/ws-ipfs/tree/master/packages/ipfs-http-client#readme",
  "bugs": {
    "url": "https://github.com/bluelovers/ws-ipfs/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bluelovers/ws-ipfs.git",
    "directory": "packages/ipfs-http-client"
  },
  "license": "ISC",
  "author": "",
  "main": "index.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "coverage": "npx nyc yarn run test",
    "lint": "npx eslint **/*.ts",
    "test": "echo \"Error: no test specified\"",
    "test:jest": "jest --passWithNoTests",
    "test:jest:coverage": "yarn run test:jest -- --coverage",
    "test:jest:snapshot": "yarn run test:jest -- -u",
    "test:mocha": "npx mocha --require ts-node/register \"!(node_modules)/**/*.{test,spec}.{ts,tsx}\"",
    "test:snapshot": "yarn run test -- -u",
    "test:tsd": "ynpx tsd",
    "npm:publish": "npm publish",
    "preversion": "yarn run test",
    "prepublish:lockfile": "npx sync-lockfile .",
    "prepublishOnly_": "yarn run ncu && yarn run sort-package-json && yarn run test",
    "postpublish_": "git commit -m \"chore(release): publish\" .",
    "ncu": "npx yarn-tool ncu -u",
    "sort-package-json": "npx yarn-tool sort",
    "tsc:default": "tsc -p tsconfig.json",
    "tsc:esm": "tsc -p tsconfig.esm.json",
    "tsc:showConfig": "ynpx get-current-tsconfig -p"
  },
  "dependencies": {
    "@lazy-ipfs/check-ipfs-connect": "^1.0.9",
    "ipfs-env": "^1.0.19",
    "ipfs-types": "^1.0.46",
    "ts-type": "^3.0.1",
    "tslib": "^2"
  },
  "devDependencies": {
    "@types/bl": "^5.0.2",
    "bl": "^6.0.7"
  },
  "peerDependencies": {
    "ipfs-http-client": "*"
  },
  "publishConfig": {
    "access": "public"
  },
