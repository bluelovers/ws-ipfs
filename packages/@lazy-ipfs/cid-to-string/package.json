    "cid",
    "dweb",
    "node",
    "webrtc",
    "swarm",
    "pubsub",
    "p2p",
    "ipns",
    "ipld"
  ],
  "homepage": "https://github.com/bluelovers/ws-ipfs/tree/master/packages/@lazy-ipfs/cid-to-string#readme",
  "bugs": {
    "url": "https://github.com/bluelovers/ws-ipfs/issues"
